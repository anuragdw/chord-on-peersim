package peersim.chord;

import java.math.*;
import peersim.core.*;

public class UpdateMessage implements ChordMessage {

	
	private int n;
	private BigInteger target;
	private long eTime;  


	public UpdateMessage( int n , BigInteger target, long time) {
		this.n = n;
		this.target = target ;
		this.eTime = time;
	}

	public int getN() {
		return n;
	}

	public BigInteger getTarget() {
		return target;
	}

	public long getETime() {
		return eTime;
	}



}
