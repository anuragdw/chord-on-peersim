package peersim.chord;

import java.math.*;
import peersim.core.*;

public class TimeOutIndexMessage implements ChordMessage {


	private int k;
	private boolean isSource;


	public TimeOutIndexMessage( int k, boolean b) {
		this.k = k;
		this.isSource = b;
		
	}

	public int getK () {
		return k;
	}

	public boolean getIsSource() {
		return isSource;
	}

}
